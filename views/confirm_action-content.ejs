<h1><%= title %></h1>

<p><strong>This action requires a password</strong></p>

<div class="row">
  <form class="col s12" action="" method="post">
    <div class="input-field col s12">
      <input
        id="passwordInput"
        name="passwordInput"
        type="password"
        class="validate"
      />
      <label for="passwordInput" name="passwordInput" class="black-text"
        >Enter password</label
      >
    </div>

    <button type="submit" class="modal-close waves-effect waves-green btn">
      Submit
    </button>
  </form>
</div>
