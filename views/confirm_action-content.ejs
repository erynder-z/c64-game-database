<h1><%= title %></h1>

<p><strong>This action requires a password</strong></p>

<div class="row">
  <form class="col s12" action="" method="post">
    <div class="input-field col s12">
      <input
        id="passwordInput"
        name="passwordInput"
        type="password"
        class="validate"
      />
      <label for="passwordInput" name="passwordInput" class="black-text"
        >Enter password</label
      >
    </div>

    <button type="submit" class="modal-close waves-effect waves-green btn">
      Submit
    </button>
    <% if (typeof errors != "undefined") { for(const [i, val] of
    errors.entries()) { if (i < errors.length-1) { %>
    <p><%=val.msg%></p>
    <% } else { %>
    <p><%=val.msg%></p>
    <% }}} %>
  </form>
</div>
